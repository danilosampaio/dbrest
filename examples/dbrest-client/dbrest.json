/*
	postgresql connection example
*/
{
	"dialect": "postgresql",
	"connection": {
		"dialect": "postgresql",
		"server": "localhost",
		"port": "5432",
		"database": "postgres",
		"user": "postgres",
		"password": "postgres"
	},
	"modelsDir": "models",
	"prefix": "dbrest"
}

